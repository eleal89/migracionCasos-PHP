    <table name="triggers">
      <record>
        <tri_uid>6406159155d1bb98ae11a39033220271</tri_uid>
        <tri_title>listar documentos</tri_title>
        <tri_description></tri_description>
        <pro_uid>3978577155cf6a916a98047021975914</pro_uid>
        <tri_type>SCRIPT</tri_type>
        <tri_webbot><![CDATA[@@mfl_documentos='';

@@queryDocumentos = "(SELECT DISTINCT APP_DOCUMENT.APP_DOC_UID AS APP, CONTENT.CON_VALUE AS ARC,
								  CONCAT(USERS.USR_FIRSTNAME,' ',USERS.USR_LASTNAME) AS NOM,
								  APP_DOCUMENT.APP_DOC_CREATE_DATE AS FEC, APP_DOCUMENT.APP_DOC_TYPE,
								  CASE WHEN APP_DOCUMENT.APP_DOC_TYPE = 'INPUT' THEN 'Adjuntado' 
								  WHEN APP_DOCUMENT.APP_DOC_TYPE = 'ATTACHED' THEN 'Adjuntado' 
								  ELSE 'Generado' END AS TIPO_DOC
					  FROM APP_DOCUMENT
					  INNER JOIN USERS ON (APP_DOCUMENT.USR_UID = USERS.USR_UID)
					  INNER JOIN CONTENT ON (APP_DOCUMENT.APP_DOC_UID = CONTENT.CON_ID)
					  WHERE CONTENT.CON_CATEGORY = 'APP_DOC_FILENAME' 
					  AND APP_DOCUMENT.APP_DOC_STATUS ='ACTIVE' 
					  AND (APP_DOCUMENT.APP_DOC_TYPE = 'ATTACHED' 
					  		OR APP_DOCUMENT.APP_DOC_TYPE = 'INPUT' 
					  		OR APP_DOCUMENT.APP_DOC_TYPE = 'OUTPUT') 
					  AND APP_DOCUMENT.APP_UID IN ('".@@APPLICATION."'))";

$application_ = @@APPLICATION;
$parentUid = '';

do{
	$queryParent = "SELECT APP_PARENT 
			FROM SUB_APPLICATION
			WHERE APP_UID = '".$application_."'";
	$resultParent = executeQuery ($queryParent);	

	if ($resultParent[1]['APP_PARENT'] != ''){ // viene de otro proceso

		$parentUid = $resultParent[1]['APP_PARENT'];
		@@queryDocumentos .= " UNION
							(SELECT DISTINCT APP_DOCUMENT.APP_DOC_UID AS APP, CONTENT.CON_VALUE AS ARC,
									  CONCAT(USERS.USR_FIRSTNAME,' ',USERS.USR_LASTNAME) AS NOM,
									  APP_DOCUMENT.APP_DOC_CREATE_DATE AS FEC, APP_DOCUMENT.APP_DOC_TYPE,
									  CASE WHEN APP_DOCUMENT.APP_DOC_TYPE = 'INPUT' THEN 'Adjuntado' 
									  WHEN APP_DOCUMENT.APP_DOC_TYPE = 'ATTACHED' THEN 'Adjuntado' 
									  ELSE 'Generado' END AS TIPO_DOC
							  FROM APP_DOCUMENT
							  INNER JOIN USERS ON (APP_DOCUMENT.USR_UID = USERS.USR_UID)
							  INNER JOIN CONTENT ON (APP_DOCUMENT.APP_DOC_UID = CONTENT.CON_ID)
							  WHERE CONTENT.CON_CATEGORY = 'APP_DOC_FILENAME' 
							  AND APP_DOCUMENT.APP_DOC_STATUS ='ACTIVE' 
							  AND (APP_DOCUMENT.APP_DOC_TYPE = 'ATTACHED' 
							  		OR APP_DOCUMENT.APP_DOC_TYPE = 'INPUT' 
							  		OR APP_DOCUMENT.APP_DOC_TYPE = 'OUTPUT') 
							  AND APP_DOCUMENT.APP_UID IN ('".$parentUid."')) ";

		$application_ = $parentUid;
	} else {
		$parentUid = '';
	}

}while ($parentUid != '');

@@queryDocumentos .= " ORDER BY FEC ASC";
$resultDocumentos = executeQuery(@@queryDocumentos);
 
$inpHtml = "<div class='boxTop'>
				<div class='a'></div>
				<div class='b'></div>
				<div class='c'></div>
			</div>
			<div class='content'>";
$inpHtml = "<style type='text/css'>
			.myOtherTable { background-color:white;border-collapse:collapse;color:#000;font-size:11px; }
			.myOtherTable th { background-color:#dddddd;color:rgb(85, 81, 81);width:25%;}
			.myOtherTable td, .myOtherTable th { padding:5px;border:0; }
			.myOtherTable td { border-bottom:1px dotted #dddddd; }
			</style>";				
$inpHtml .= "<table width='100%'  class='myOtherTable'>";
if (count($resultDocumentos) == 0)
{
	$inpHtml .= "<tr>
				<th width='100%'> NO EXISTEN DOCUMENTOS</th>
				</tr>";
}else{
   	$inpHtml .= "<tr>
				<th width='25%'>DESCARGAR ARCHIVO</th>
				<th width='25%'>ADJUNTADO POR</th>
				<th width='25%'>TIPO DOCUMENTO</th>
				<th width='25%'>FECHA</th>
				</tr>";
	foreach ($resultDocumentos as $valor)
	{
		$aux = pathinfo($valor['ARC']);
		$extension = $aux['extension'];

        list($ano,$mes,$dia) = explode("-",substr($valor['FEC'],0,10));
        $hora = substr($valor['FEC'], 11,19);

        $valor['FEC'] = $dia."-".$mes."-".$ano. " ". $hora;

        $col2= '';
        if ($valor['APP_DOC_TYPE'] == "OUTPUT") {
			$col2 = "<a href='../cases/cases_ShowOutputDocument?a=" . $valor['APP'] . "&v=&ext=doc'>" . $valor['ARC'] . "</a>";
	    } else {
	    	$col2 = "<a href='../cases/cases_ShowDocument?a=" . $valor['APP'] . "'>" . $valor['ARC'] . "</a>";
		}
      
		$inpHtml .= "<tr>
						<td>".$col2."</td>
						<td>".$valor['NOM']."</td>
						<td>".$valor['TIPO_DOC']."</td>
						<td>".$valor['FEC']."</td>
					</tr>";
 
	}
		
}
$inpHtml .= "</table>";
	$inpHtml .= "</div>
				<div class='boxBottom'>
				<div class='a'></div>
				<div class='b'></div>
				<div class='c'></div></div>";

@@DESCARGAR_DOCUMENTOS = $inpHtml;]]></tri_webbot>
        <tri_param></tri_param>
      </record>
    </table>
