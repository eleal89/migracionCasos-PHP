        <dyn_content><![CDATA[{"name":"Datos instituciones","description":"","items":[{"type":"form","variable":"","var_uid":"","dataType":"","id":"8339329465d7818ea742bd6048342125","name":"Datos instituciones","description":"","mode":"edit","script":{"type":"js","code":"\nfunTipoBusqInstitucion();\n\n\nfunction funTipoBusqInstitucion(){\n  _tipo_busqueda_inst =  $(\"#cbo_tipo_busqueda_inst\").getValue();\n  $(\"#grid_inst_encontradas\").hide();\n  switch(_tipo_busqueda_inst){\n    case '1'://buscar por nombre      \n        $(\"#cbo_tipo_inst\").show();\n        $(\"#txt_nom_inst\").show();  \n        $(\"#cbo_dep_inst\").show(); \n        $(\"#cbo_prov_inst\").show(); \n        $(\"#cbo_dist_inst\").show();   \n        $(\"#btn_buscar_inst\").show(); \n      $(\"#btn_registrar_inst\").hide();\n      $(\"#txt_dir_inst\").hide();\n      $(\"#cbo_natu_inst\").hide();\n      break;\n    case '2'://buscar por código\n      $(\"#cbo_tipo_inst\").show();\n        $(\"#txt_nom_inst\").show();  \n        $(\"#cbo_dep_inst\").hide(); \n        $(\"#cbo_prov_inst\").hide(); \n        $(\"#cbo_dist_inst\").hide(); \n        $(\"#btn_buscar_inst\").show(); \n      $(\"#btn_registrar_inst\").hide();\n      $(\"#txt_dir_inst\").hide();\n      $(\"#cbo_natu_inst\").hide();\n      break;    \n    case '3'://registro manual  \n        $(\"#cbo_tipo_inst\").show();\n        $(\"#txt_nom_inst\").show();  \n        $(\"#cbo_dep_inst\").show(); \n        $(\"#cbo_prov_inst\").show(); \n        $(\"#cbo_dist_inst\").show(); \n        $(\"#btn_buscar_inst\").hide(); \n      $(\"#btn_registrar_inst\").show();\n      $(\"#txt_dir_inst\").show();\n      $(\"#cbo_natu_inst\").show();\n      break;\n    default: //ninguno\n      $(\"#cbo_tipo_inst\").hide();\n        $(\"#txt_nom_inst\").hide();  \n        $(\"#cbo_dep_inst\").hide(); \n        $(\"#cbo_prov_inst\").hide(); \n        $(\"#cbo_dist_inst\").hide(); \n        $(\"#btn_buscar_inst\").hide(); \n      $(\"#btn_registrar_inst\").hide();\n      $(\"#txt_dir_inst\").hide();\n      $(\"#cbo_natu_inst\").hide();\n      break;\n  }\n}\n$(\"#cbo_tipo_busqueda_inst\").setOnchange(funTipoBusqInstitucion);\n\nfunction clickBuscarInstitucion(){\n  _tipo_busqueda_inst =  $(\"#cbo_tipo_busqueda_inst\").getValue();\n  switch(_tipo_busqueda_inst){\n    case '1'://buscar por nombre          \n      var _tipo_inst_busq = $(\"#cbo_tipo_inst\").getValue();\n        if (_tipo_inst_busq.length == 0) {\n          alert(\"Seleccione el tipo de institución (IPRESS, IAFAS O UGIPRESS).\");\n          getFieldById(\"cbo_tipo_inst\").setFocus();\n        break;\n        }\n        if (_tipo_inst_busq != '1' && _tipo_inst_busq != '2' && _tipo_inst_busq != '3') {\n          alert(\"La busqueda por nombre solo está disponible para IPRESS, IAFAS O UGIPRESS.\");\n        getFieldById(\"cbo_tipo_inst\").setFocus();\n        break;\n        }\n      var _institucion_busq = $(\"#txt_nom_inst\").getValue();\n        if (_institucion_busq.length < 4) {\n          alert(\"El nombre de la institución debe tener 4 caracteres como mínimo.\");\n        getFieldById(\"txt_nom_inst\").setFocus();\n        break;\n        }\n        funGetBuscaInstituciones(_tipo_busqueda_inst);\n    break;\n    case '2'://buscar por código\n      var _tipo_inst_busq = $(\"#cbo_tipo_inst\").getValue();\n        if (_tipo_inst_busq.length == 0) {\n          alert(\"Seleccione el tipo de institución (IPRESS, IAFAS O UGIPRESS).\");\n        getFieldById(\"cbo_tipo_inst\").setFocus();\n        break;\n        }\n        if (_tipo_inst_busq != '1' && _tipo_inst_busq != '2' && _tipo_inst_busq != '3') {\n          alert(\"La busqueda por código solo está disponible para IPRESS, IAFAS O UGIPRESS.\");\n        getFieldById(\"cbo_tipo_inst\").setFocus();\n        break;\n        }\n      var _institucion_busq = $(\"#txt_nom_inst\").getValue();\n        if (_institucion_busq.length < 1) {\n          alert(\"Debe registrar el código de la institución.\");\n        getFieldById(\"txt_nom_inst\").setFocus();\n        break;\n        }\n        funGetBuscaInstituciones(_tipo_busqueda_inst);\n    break;    \n    case '3'://registro manual        \n      break;\n    default: //ninguno\n      alert(\"Debe seleccionar una opción.\");\n      getFieldById(\"cbo_tipo_busqueda_inst\").setFocus( ) ; \n    break;\n  }\n}\n$(\"#btn_buscar_inst\").find(\"button\").click(clickBuscarInstitucion);\n\nfunction funGetBuscaInstituciones(_tipo_busqueda_inst){\n  //llamar servicio busqueda\n  $('#'+$(\"#uid_formulario\").getValue()).showFormModal();\n  //llamar servicio\n  var _tipo_inst_busq = $(\"#cbo_tipo_inst\").getValue();\n  var _institucion_busq = $(\"#txt_nom_inst\").getValue();\n  var _ubigeo = \"&dep=\"+$(\"#cbo_dep_inst\").getValue()+\"&prov=\"+$(\"#cbo_prov_inst\").getValue()+\"&dist=\"+$(\"#cbo_dist_inst\").getValue();\n  var Url =  \"/webservices/ws_procesos/obtenerDatosInstituciones.php?institucion=\"+_institucion_busq+\"&tipoInst=\"+_tipo_inst_busq+\"&tipoBusqueda=\"+_tipo_busqueda_inst+_ubigeo;\n  $.ajax({\n          url: Url,\n          type: 'GET' })\n          .done( function(result) {  \n            clearGrid (\"#grid_inst_encontradas\");\n            if (result.CO_RESPUESTA == 1) {\n              console.log(\"countInstEncontradas--> \" + result.INSTITUCIONES.length);\n              var countInst = 0;\n              if(result.INSTITUCIONES.length > 40) countInst = 40;\n              else countInst = result.INSTITUCIONES.length;\n              console.log(\"countInstMostradas--> \" + countInst);\n              for (var i = 1; i <= countInst; i++) {\n                ubigeo = result.INSTITUCIONES[i-1].DESC_DEPARTAMENTO+\"-\"+\n                      result.INSTITUCIONES[i-1].DESC_PROVINCIA+\"-\"+\n                      result.INSTITUCIONES[i-1].DESC_DISTRITO;\n                var aNewRow  = [{ value : result.INSTITUCIONES[i-1].CODIGO} , \n                          { value : result.INSTITUCIONES[i-1].NOMBRE} , \n                        { value : result.INSTITUCIONES[i-1].NOMBRE_COMER} ,\n                          { value : ubigeo} ,  \n                          { value : result.INSTITUCIONES[i-1].CLASIFICACION} ,  \n                          { value : result.INSTITUCIONES[i-1].INSTITUCION},  \n                          { value : result.INSTITUCIONES[i-1].TIPO},  \n                          { value : \"0\" }] ; \n                $ (\"#grid_inst_encontradas\").addRow (aNewRow) ;                       \n              }\n              $(\"#grid_inst_encontradas\").deleteRow(1);//eliminar la primera fila vacia\n              $(\"#grid_inst_encontradas\").show();\n              $(\"#btn_registrar_inst\").show();\n            } else { \n              alert(result.MSJ_RESPUESTA);\n              $(\"#grid_inst_encontradas\").hide(); \n              $(\"#btn_registrar_inst\").hide();\n            }\n\n          }).fail( function() {\n            alert(\"Servicio no disponible\");\n            $(\"#grid_inst_encontradas\").hide(); \n            $(\"#btn_registrar_inst\").hide();\n          }).always( function() {    \n            $('#'+$(\"#uid_formulario\").getValue()).hideFormModal();  \n          });  \n}\n\nfunction funRegistrarInstitucion(){\n  _tipo_busqueda_inst =  $(\"#cbo_tipo_busqueda_inst\").getValue();\n  \n  if(_tipo_busqueda_inst == '3'){ //registro manual \n    var _tipo_inst_busq = $(\"#cbo_tipo_inst\").getValue();\n      if (_tipo_inst_busq.length == 0) {\n        alert(\"Seleccione el tipo de institución.\");\n      getFieldById(\"cbo_tipo_inst\").setFocus();\n      return false;\n      }\n    var _institucion_busq = $(\"#txt_nom_inst\").getValue();\n      if (_institucion_busq.length < 4) {\n        alert(\"El nombre de la institución debe tener 4 caracteres como mínimo.\");\n      getFieldById(\"txt_nom_inst\").setFocus();\n      return false;\n      }\n    var _institucion_dist = $(\"#cbo_dist_inst\").getValue();\n      if (_institucion_dist.length < 1) {\n        alert(\"Debe seleccionar Departamento, Provincia y Distrito de la institucón\");\n      getFieldById(\"cbo_dep_inst\").setFocus();\n      return false;\n      }\n    var _cbo_natu_inst = $(\"#cbo_natu_inst\").getValue();\n      if (_cbo_natu_inst.length < 1) {\n        alert(\"Debe seleccionar naturaleza de la institucón\");\n      getFieldById(\"cbo_natu_inst\").setFocus();\n      return false;\n      }\n      var ubigeo_institucion = $(\"#cbo_dep_inst\").getText() +\"-\"+ \n                    $(\"#cbo_prov_inst\").getText()+\"-\"+\n                    $(\"#cbo_dist_inst\").getText();\n    var direccion  = $(\"#txt_dir_inst\").getValue();\n      if (direccion.length > 0) { \n        _institucion_busq = _institucion_busq+ \"\\nDirección: \"+direccion;\n      }           \n      var aData = [{ value : \"-\"} , \n            { value : _institucion_busq} , \n                { value : ubigeo_institucion} ,  \n                { value : \"-\"} ,  \n                { value : $(\"#cbo_natu_inst\").getText()},  \n                { value : $(\"#cbo_tipo_inst\").getText()}] ; \n    $(\"#grid_institucionesInvolucradas\").addRow (aData) ; \n    //LIMPIAR LOS CAMPOS    \n      $(\"#cbo_tipo_busqueda_inst\").setValue(\"\");\n      $(\"#cbo_tipo_inst\").setValue(\"\");\n      $(\"#txt_nom_inst\").setValue(\"\");\n      $(\"#cbo_dep_inst\").setValue(\"\");\n      $(\"#cbo_natu_inst\").setValue(\"\");\n      $(\"#txt_dir_inst\").setValue(\"\");\n\n    //validar si la primera fila está vacia \n    var nombreInst = $('#grid_institucionesInvolucradas').getValue(1,2);//nombre inst\n    if(nombreInst == ''){\n          $(\"#grid_institucionesInvolucradas\").deleteRow(1);//eliminar la primera fila vacia        \n    }\n  } else { //registrar de grilla\n    var rows = $(\"#grid_inst_encontradas\").getNumberRows();   \n      var contarSeleccionados = 0;\n      var fila = 0;\n      for (var i = 1 ; i <= rows ; i++) { \n          if ($('#grid_inst_encontradas').getValue(i,8) == '1'){          \n              contarSeleccionados++;\n              fila = i;\n        }\n      } \n      if (contarSeleccionados == 0) {\n          alert(\"Debe seleccionar una institución\");\n          return false;\n      } else if (contarSeleccionados > 1) {\n          alert(\"Debe seleccionar solo una institución\");\n          return false;\n      } else if (contarSeleccionados == 1) {\n        if($('#grid_inst_encontradas').getValue(fila,2) != ''){\n      \t\tnomb_inst = $('#grid_inst_encontradas').getValue(fila,2);\n      \t} else {\n      \t\tnomb_inst = $('#grid_inst_encontradas').getValue(fila,3);\n      \t}\n        var aData = [{ value : $('#grid_inst_encontradas').getValue(fila,1)} , \n            \t{ value :nomb_inst} ,   \n                { value : $('#grid_inst_encontradas').getValue(fila,4)} ,  \n                { value : $('#grid_inst_encontradas').getValue(fila,5)},  \n                { value : $('#grid_inst_encontradas').getValue(fila,6)},  \n                { value : $('#grid_inst_encontradas').getValue(fila,7)}] ; \n      $(\"#grid_institucionesInvolucradas\").addRow (aData) ;  \n      $('#grid_inst_encontradas').setValue('0',fila,7); //eliminar el check\n      //LIMPIAR LOS CAMPOS    \n        $(\"#cbo_tipo_busqueda_inst\").setValue(\"\");\n        $(\"#cbo_tipo_inst\").setValue(\"\");\n        $(\"#txt_nom_inst\").setValue(\"\");\n        $(\"#cbo_dep_inst\").setValue(\"\");\n        $(\"#txt_dir_inst\").setValue(\"\");\n      //validar si la primera fila está vacia \n      var nombreInst = $('#grid_institucionesInvolucradas').getValue(1,2);//nombre inst\n      if(nombreInst == ''){\n            $(\"#grid_institucionesInvolucradas\").deleteRow(1);//eliminar la primera fila vacia        \n      }   \n      } \n  }       \n}\n$(\"#btn_registrar_inst\").find(\"button\").click(funRegistrarInstitucion);\n\nfunction clearGrid (_grid){\n    //eliminar filas menos la primera:\n    var rows = $(_grid).getNumberRows();\n    for (var i=1; i < rows; i++) {\n        $(_grid).deleteRow();\n    }\n    //borrar datos de la 1ra fila:\n    var aValues = $(_grid).getValue();\n    for (var i=1; i <= aValues[0].length; i++) {\n        $(_grid).setValue(\"\", 1, i);\n    }\n};\n\n\nfunction validaDatosInstituciones(){\n  var _tipo_solicitud = $(\"#TIPO_SOLICITUD\").getValue();   \n  var rows_isnt = $(\"#grid_institucionesInvolucradas\").getNumberRows(); \n  if(rows_isnt < 1){\n    if(_tipo_solicitud == '1'){\n      if(!confirm(\"No ha registrado instituciones involucradas ¿Desea continuar? \")) return false;\n    }else \n    if(_tipo_solicitud == '2'){\n      alert(\"Debe registrar la institución involucrada.\"); return false;   \n    }else \n    if(_tipo_solicitud == '3'){\n      alert(\"Debe registrar la institución involucrada.\"); return false;        \n    }else {\n      alert(\"Debe registrar la institución involucrada.\"); return false;   \n    } \n  }\n  return true;\n}\n\n                    \n$(\"#grid_inst_encontradas\").find(\"p.pmdynaform-grid-title\").css({\"text-decoration-line\": \"underline\",\"text-align\": \"left\"});\n$(\"#grid_institucionesInvolucradas\").find(\"p.pmdynaform-grid-title\").css({\"text-decoration-line\": \"underline\",\"text-align\": \"left\"});\n\n$(\"#2742103485bec48b90394b1037025276\").css({\"margin-bottom\": \"15px\"}); "},"language":"es-ES","externalLibs":"","printable":false,"items":[[{"type":"dropdown","variable":"","var_uid":"","dataType":"","protectedValue":false,"id":"cbo_tipo_busqueda_inst","name":"dropdown0000000003","label":"Opciones","defaultValue":"","placeholder":"","hint":"","required":false,"requiredFieldErrorMessage":"","mode":"parent","datasource":"database","dbConnection":"workflow","dbConnectionLabel":"PM Database","sql":"","dataVariable":"","options":[{"value":"","label":"Seleccione"},{"value":"1","label":"Buscar por nombre"},{"value":"2","label":"Buscar por código"},{"value":"3","label":"Registro manual"}],"colSpan":4},{"type":"dropdown","variable":"tipo_inst","var_uid":"9305536365bec48b91a58b90